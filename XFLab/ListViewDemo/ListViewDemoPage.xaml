    <StackLayout VerticalOptions="Center" Spacing="10" Margin="20">
        <Button x:Name="BuiltinCell" Text="BuiltInCell" TextColor="Black" HorizontalOptions="FillAndExpand" Clicked="BuiltinCell_Clicked"/>
        <Button x:Name="CustomCell" Text="CustomCell" TextColor="Black" HorizontalOptions="FillAndExpand" Clicked="CustomCell_Clicked" />
        <Button x:Name="Grouping" Text="Grouping" TextColor="Black" HorizontalOptions="FillAndExpand" Clicked="Grouping_Clicked"/>
        <Button x:Name="Interactivity" Text="Interactivity" TextColor="Black" HorizontalOptions="FillAndExpand" Clicked="Interactivity_Clicked" />
    </StackLayout>
</ContentPage>
